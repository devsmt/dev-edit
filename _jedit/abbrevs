array_first_|// Return the first element in an array passing a given truth test.\nfunction array_first($array, callable $callback = null, $default = null) {\n        if (is_null($callback)) {\n            if (empty($array)) {\n                return $default;\n            }\n            foreach ($array as $item) {\n                return $item;\n            }\n        }\n        foreach ($array as $key => $value) {\n            if (call_user_func($callback, $value, $key)) {\n                return $value;\n            }\n        }\n        return $default ;\n}
